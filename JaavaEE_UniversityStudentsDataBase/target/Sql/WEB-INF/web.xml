<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>Index</servlet-name>
    <jsp-file>/index.html</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Index</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>Add</servlet-name>
    <jsp-file>/add.html</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Add</servlet-name>
    <url-pattern>/addPage</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>Del</servlet-name>
    <jsp-file>/delete.html</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Del</servlet-name>
    <url-pattern>/deletePage</url-pattern>
  </servlet-mapping>



  <servlet>
    <servlet-name>Update</servlet-name>
    <jsp-file>/update.html</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Update</servlet-name>
    <url-pattern>/updatePage</url-pattern>
  </servlet-mapping>


</web-app>
